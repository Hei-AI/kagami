llm_providers:
  openai:
    base_url: "https://api.openai.com/v1"
    api_keys:
      - "your-openai-api-key-1"
      - "your-openai-api-key-2"
    models:
      - "gpt-4o"
      - "gpt-4"
      - "gpt-3.5-turbo"
  gemini:
    base_url: "https://generativelanguage.googleapis.com/v1beta/openai/"
    api_keys:
      - "your-gemini-api-key-1"
      - "your-gemini-api-key-2"
    models:
      - "gemini-2.5-flash"
      - "gemini-1.5-pro"
      - "gemini-1.5-flash"

llm:
  model: "gpt-4o"  # 选择上面某个提供商支持的模型

napcat:
  base_url: "ws://localhost:3001"
  access_token: "your-napcat-token"
  reconnection:
    enable: true
    attempts: 10
    delay: 5000
  bot_qq: 123456789
  groups: [123456789, 987654321]

master:
  qq: 123456789  # 主人的QQ号
  nickname: "主人昵称"  # 主人的昵称

behavior:
  energy_max: 100
  energy_cost: 1
  energy_recovery_rate: 3       # 生产环境更慢的恢复速度
  energy_recovery_interval: 60
  message_handler_type: "active"
