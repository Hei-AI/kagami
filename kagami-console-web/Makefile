# kagami-console-web Makefile

.PHONY: all build clean lint install

# 默认目标
all: build

# 构建项目
build:
	@echo "构建 kagami-console-web..."
	npm run build

# 安装依赖
install:
	@echo "安装 kagami-console-web 依赖..."
	npm install

# 清理项目
clean:
	@echo "清理 kagami-console-web..."
	rm -rf dist/

# Lint 项目
lint:
	@echo "Linting kagami-console-web..."
	npm run lint
