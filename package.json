{
  "name": "kagami-workspace",
  "version": "1.0.0",
  "description": "Kagami QQ 群聊机器人",
  "private": true,
  "scripts": {
    "build": "pnpm --recursive --stream build",
    "build:bot": "pnpm --filter kagami-bot build",
    "build:web": "pnpm --filter kagami-console-web build",
    "lint": "pnpm --recursive --stream lint",
    "lint:bot": "pnpm --filter kagami-bot lint",
    "lint:web": "pnpm --filter kagami-console-web lint",
    "clean": "pnpm --recursive exec rm -rf dist"
  },
  "author": "Kisin Wen",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KisinTheFlame/kagami.git"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@stylistic/eslint-plugin": "^5.3.1",
    "@types/node": "^24.2.0",
    "eslint": "^9.33.0",
    "globals": "^16.4.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1"
  }
}
